      </div>
    </div>
    <div class="rubiks_cube_container">
      <div class="rubiks_cube rubiks_cube_0">
        <% for(var i=0; i<27; i++) {%>
          <% var classSuffixe=i; %>
            <div class="cube_container cube_container_<%= classSuffixe %>" data-pos-init="<%= i%>" data-pos="<%= i%>">
              <div class="cube_container_position cube_container_position_<%= classSuffixe %>">
                <!-- <span><%= i%></span> -->
                <figure class="cube">
                  <section class="cube-pivot">
                    <section class="cube_face cube_face_0"></section>
                    <section class="cube_face cube_face_1"></section>
                    <section class="cube_face cube_face_2"></section>
                    <section class="cube_face cube_face_3"></section>
                    <section class="cube_face cube_face_4"></section>
                    <section class="cube_face cube_face_5"></section>
                  </section>
                </figure>
              </div>
            </div>
            <% } %>
      </div>
    </div>
    <div class="ui_container">
      <div class="btns_move_grid">
        <button class="btn btn_move" data-id="L">
          L
        </button>
        <button class="btn btn_move" data-id="L'">
          L'
        </button>
        <button class="btn btn_move" data-id="U">
          U
        </button>
        <button class="btn btn_move" data-id="U'">
          U'
        </button>
        <button class="btn btn_move" data-id="B">
          B
        </button>
        <button class="btn btn_move" data-id="B'">
          B'
        </button>
        <button class="btn btn_move" data-id="F">
          F
        </button>
        <button class="btn btn_move" data-id="F'">
          F'
        </button>
        <button class="btn btn_move" data-id="D">
          D
        </button>
        <button class="btn btn_move" data-id="D'">
          D'
        </button>

        <button class="btn btn_move" data-id="R">
          R
        </button>
        <button class="btn btn_move" data-id="R'">
          R'
        </button>
        <button class="btn btn_move" data-id="X'">
          X'
        </button>
        <button class="btn btn_move" data-id="X">
          X
        </button>
        <button class="btn btn_move" data-id="Y">
          Y
        </button>
        <button class="btn btn_move" data-id="Y'">
          Y'
        </button>
        <button class="btn btn_move" data-id="Z'">
          Z'
        </button>
        <button class="btn btn_move" data-id="Z">
          Z
        </button>
      </div>
    </div>
  </div>
  <div class="dialog-container">
    <dialog class="dialog" data-id="settings">
      <div class="dialog-content">
        <button class="close-dialog" aria-label="close">
          <svg height="16px" width="16px" version="1.1" viewBox="0 0 512 512" xml:space="preserve"
            xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
            <path
              d="M443.6,387.1L312.4,255.4l131.5-130c5.4-5.4,5.4-14.2,0-19.6l-37.4-37.6c-2.6-2.6-6.1-4-9.8-4c-3.7,0-7.2,1.5-9.8,4  L256,197.8L124.9,68.3c-2.6-2.6-6.1-4-9.8-4c-3.7,0-7.2,1.5-9.8,4L68,105.9c-5.4,5.4-5.4,14.2,0,19.6l131.5,130L68.4,387.1  c-2.6,2.6-4.1,6.1-4.1,9.8c0,3.7,1.4,7.2,4.1,9.8l37.4,37.6c2.7,2.7,6.2,4.1,9.8,4.1c3.5,0,7.1-1.3,9.8-4.1L256,313.1l130.7,131.1  c2.7,2.7,6.2,4.1,9.8,4.1c3.5,0,7.1-1.3,9.8-4.1l37.4-37.6c2.6-2.6,4.1-6.1,4.1-9.8C447.7,393.2,446.2,389.7,443.6,387.1z" />
          </svg>
        </button>
        <h2 class="h2-title">Settings</h2>
        <h3 class="h3-title">Parameters</h2>
          <div class="inputs-container">
            <div class="input-spacing-container">
              <output class="input-spacing-output">.4s</output>
              <input class="input-spacing" type="range" id="transform-time-cube" name="transform-time-cube" value=".6"
                min="0.1" max="1.0" step="0.1" data-unit="s">
              <label class="input-label" for="transform-time-cube">Time to rotate</label>
            </div>
            <div class="input-spacing-container">
              <output class="input-spacing-output">4px</output>
              <input class="input-spacing" type="range" id="space-between-cubes" name="space-between-cubes" value="4"
                min="0" max="50" step="1" data-unit="px">
              <label class="input-label" for="space-between-cubes">Space between cubes</label>
            </div>
            <div class="input-spacing-container">
              <output class="input-spacing-output">4px</output>
              <input class="input-spacing" type="range" id="cube-border-radius" name="cube-border-radius" value="4"
                min="0" max="50" step="1" data-unit="px">
              <label class="input-label" for="cube-border-radius">Border radius</label>
            </div>
            <div class="input-spacing-container">
              <output class="input-spacing-output">4px</output>
              <input class="input-spacing" type="range" id="unused-cube-border-radius" name="unused-cube-border-radius"
                value="4" min="0" max="50" step="1" data-unit="px">
              <label class="input-label" for="unused-cube-border-radius">Border radius unused</label>
            </div>
          </div>
          <div class="randomize-inputs">
            <div class="btn randomize-btn">
              <svg width="512" height="512" viewBox="0 0 512 512"
                style="color:currentColor" xmlns="http://www.w3.org/2000/svg" class="randomize-icon">
                <rect width="512" height="512" x="0" y="0" rx="30" fill="transparent" stroke="transparent"
                  stroke-width="0" stroke-opacity="100%" paint-order="stroke"></rect><svg width="256px" height="256px"
                  viewBox="0 0 32 24" fill="currentColor" x="128" y="128" role="img"
                  style="display:inline-block;vertical-align:middle" xmlns="http://www.w3.org/2000/svg">
                  <g fill="currentColor">
                    <path fill="currentColor"
                      d="M24.983 8.539V6.054h-4.902l-3.672 5.945l-2.099 3.414l-3.24 5.256c-.326.51-.889.844-1.53.845H0v-3.568h8.538L12.211 12l2.099-3.414l3.24-5.256a1.812 1.812 0 0 1 1.525-.845h5.904V0l7.417 4.27l-7.417 4.27z" />
                    <path fill="currentColor"
                      d="m12.902 6.316l-.63 1.022l-1.468 2.39l-2.265-3.675H.001V2.485h9.54a1.813 1.813 0 0 1 1.526.838l.004.007l1.836 2.985zM24.983 24v-2.485h-5.904a1.809 1.809 0 0 1-1.521-.838l-.004-.007l-1.836-2.985l.63-1.022l1.468-2.39l2.264 3.675h4.902v-2.485l7.417 4.27l-7.417 4.27z" />
                  </g>
                </svg>
              </svg>
            </div>
          </div>
          <h3 class="h3-title">Colors - Alpha</h2>
           
            <div class="inputs-container">
              <div class="input-color-container">
                <input class="input-color" type="color" id="white" name="white" value="#ffffff" />
                <input class="input-alpha" type="range" id="white-alpha" name="white-alpha" value="255.0" min="0.0"
                  max="255.0" step="1.0" />
                <label class="input-label" for="white">white</label>
              </div>
              <div class="input-color-container">
                <input class="input-color" type="color" id="blue" name="blue" value="#0000ff" />
                <input class="input-alpha" type="range" id="blue-alpha" name="blue-alpha" value="255.0" min="0.0"
                  max="255.0" step="1.0" />
                <label class="input-label" for="blue">Blue</label>
              </div>
              <div class="input-color-container">
                <input class="input-color" type="color" id="red" name="red" value="#ff0000" />
                <input class="input-alpha" type="range" id="red-alpha" name="red-alpha" value="255.0" min="0.0"
                  max="255.0" step="1.0" />
                <label class="input-label" for="red">Red</label>
              </div>
              <div class="input-color-container">
                <input class="input-color" type="color" id="yellow" name="yellow" value="#ffff00" />
                <input class="input-alpha" type="range" id="yellow-alpha" name="yellow-alpha" value="255.0" min="0.0"
                  max="255.0" step="1.0" />
                <label class="input-label" for="yellow">Yellow</label>
              </div>
              <div class="input-color-container">
                <input class="input-color" type="color" id="green" name="green" value="#008000" />
                <input class="input-alpha" type="range" id="green-alpha" name="green-alpha" value="255.0" min="0.0"
                  max="255.0" step="1.0" />
                <label class="input-label" for="green">Green</label>
              </div>
              <div class="input-color-container">
                <input class="input-color" type="color" id="orange" name="orange" value="#ffa500" />
                <input class="input-alpha" type="range" id="orange-alpha" name="orange-alpha" value="255.0" min="0.0"
                  max="255.0" step="1.0" />
                <label class="input-label" for="orange">Orange</label>
              </div>
              <div class="input-color-container">
                <input class="input-color" type="color" id="unused_face_color" name="unused_face_color"
                  value="#313131" />
                <input class="input-alpha" type="range" id="unused_face_color-alpha" name="unused_face_color-alpha"
                  value="255.0" min="0.0" max="255.0" step="1.0" />
                <label class="input-label" for="unused_face_color">Unused Faces</label>
              </div>
              <div class="input-color-container">
                <input class="input-color" type="color" id="border_color" name="border_color" value="#000000" />
                <input class="input-alpha" type="range" id="border_color-alpha" name="border_color-alpha" value="255.0"
                  min="0.0" max="255.0" step="1.0" />
                <label class="input-label" for="border_color">Border Color</label>
              </div>
            </div>
            <div class="btns-dialog-container">
              <button class="btn btn-dialog dialog-settings-btn-save">Save</button>
              <button class="btn btn-dialog dialog-settings-btn-reset">Reset</button>
            </div>
      </div>
    </dialog>
  </div>
</body>

</html>