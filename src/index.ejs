    <div class="rubiks_cube_container">
      <div class="rubiks_cube rubiks_cube_0">
        <% for(var i=0; i<27; i++) {%>
          <% var classSuffixe=i; %>
            <div class="cube_container cube_container_<%= classSuffixe %>" data-pos-init="<%= i%>" data-pos="<%= i%>">
              <div class="cube_container_position cube_container_position_<%= classSuffixe %>">
                <!-- <span><%= i%></span> -->
                <figure class="cube">
                  <section class="cube-pivot">
                    <section class="cube_face cube_face_0"></section>
                    <section class="cube_face cube_face_1"></section>
                    <section class="cube_face cube_face_2"></section>
                    <section class="cube_face cube_face_3"></section>
                    <section class="cube_face cube_face_4"></section>
                    <section class="cube_face cube_face_5"></section>
                  </section>
                </figure>
              </div>
            </div>
            <% } %>
      </div>
    </div>
    <div class="ui_container">
      <div class="btns_move_grid">
        <button class="btn btn_move" data-id="U">
          U
        </button>
        <button class="btn btn_move" data-id="U'">
          U'
        </button>
        <button class="btn btn_move" data-id="D">
          D
        </button>
        <button class="btn btn_move" data-id="D'">
          D'
        </button>
        <button class="btn btn_move" data-id="F">
          F
        </button>
        <button class="btn btn_move" data-id="F'">
          F'
        </button>
        <button class="btn btn_move" data-id="B">
          B
        </button>
        <button class="btn btn_move" data-id="B'">
          B'
        </button>
        <button class="btn btn_move" data-id="R">
          R
        </button>
        <button class="btn btn_move" data-id="R'">
          R'
        </button>
        <button class="btn btn_move" data-id="L">
          L
        </button>
        <button class="btn btn_move" data-id="L'">
          L'
        </button>

        <button class="btn btn_move" data-id="X">
          X
        </button>
        <button class="btn btn_move" data-id="X'">
          X'
        </button>
        <button class="btn btn_move" data-id="Y">
          Y
        </button>
        <button class="btn btn_move" data-id="Y'">
          Y'
        </button>
        <button class="btn btn_move" data-id="Z">
          Z
        </button>
        <button class="btn btn_move" data-id="Z'">
          Z'
        </button>

      </div>
    </div>
  </div>
  <div class="dialog-container">
    <dialog class="dialog" data-id="settings">
      <button class="close-dialog" aria-label="close">
        <svg
          height="16px"  width="16px" version="1.1" viewBox="0 0 512 512"
          xml:space="preserve" xmlns="http://www.w3.org/2000/svg"
          xmlns:xlink="http://www.w3.org/1999/xlink">
          <path
            d="M443.6,387.1L312.4,255.4l131.5-130c5.4-5.4,5.4-14.2,0-19.6l-37.4-37.6c-2.6-2.6-6.1-4-9.8-4c-3.7,0-7.2,1.5-9.8,4  L256,197.8L124.9,68.3c-2.6-2.6-6.1-4-9.8-4c-3.7,0-7.2,1.5-9.8,4L68,105.9c-5.4,5.4-5.4,14.2,0,19.6l131.5,130L68.4,387.1  c-2.6,2.6-4.1,6.1-4.1,9.8c0,3.7,1.4,7.2,4.1,9.8l37.4,37.6c2.7,2.7,6.2,4.1,9.8,4.1c3.5,0,7.1-1.3,9.8-4.1L256,313.1l130.7,131.1  c2.7,2.7,6.2,4.1,9.8,4.1c3.5,0,7.1-1.3,9.8-4.1l37.4-37.6c2.6-2.6,4.1-6.1,4.1-9.8C447.7,393.2,446.2,389.7,443.6,387.1z" />
        </svg>
      </button>
      <h2 class="h2-title">Settings</h2>
      <h3 class="h3-title">Colors:</h2>
        <div class="inputs-container">
          <div class="input-color-container">
            <input class="input-color" type="color" id="white" name="white" value="#ffffff" />
            <label class="input-label" for="white">white</label>
          </div>
          <div class="input-color-container">
            <input class="input-color" type="color" id="blue" name="blue" value="#0000ff" />
            <label class="input-label" for="blue">Blue</label>
          </div>
          <div class="input-color-container">
            <input class="input-color" type="color" id="red" name="red" value="#ff0000" />
            <label class="input-label" for="red">Red</label>
          </div>
          <div class="input-color-container">
            <input class="input-color" type="color" id="yellow" name="yellow" value="#ffff00" />
            <label class="input-label" for="yellow">Yellow</label>
          </div>
          <div class="input-color-container">
            <input class="input-color" type="color" id="green" name="green" value="#008000" />
            <label class="input-label" for="green">Green</label>
          </div>
          <div class="input-color-container">
            <input class="input-color" type="color" id="orange" name="orange" value="#ffa500" />
            <label class="input-label" for="orange">Orange</label>
          </div>

        </div>
        <div class="btns-dialog-container">
          <button class="btn dialog-settings-btn-save">Save</button>
          <button class="btn dialog-settings-btn-reset">Reset</button>
        </div>
    </dialog>
  </div>
</body>

</html>